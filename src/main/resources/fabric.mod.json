{
	"schemaVersion": 1,
	"id": "second-mod",
	"version": "${version}",
	"name": "second-mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/second-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.pashmi.Secondmod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.pashmi.SecondmodClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"second-mod.mixins.json",
		{
			"config": "second-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}